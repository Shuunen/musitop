{
  "name": "musitop",
  "version": "3.2.4",
  "description": "Musitop delete music you dont like",
  "main": "server.js",
  "dependencies": {
    "colorable": "^1.0.5",
    "config-prompt": "^1.1.0",
    "electron": "^1.7.3",
    "eslint": "^3.13.1",
    "express": "^4.14.1",
    "ip": "^1.1.4",
    "jscs": "^3.0.7",
    "minimist": "^1.2.0",
    "morgan": "^1.8.1",
    "musicmetadata": "^2.0.5",
    "node-notifier": "^5.1.2",
    "node-vibrant": "^3.0.0-alpha.2",
    "os": "^0.1.1",
    "sharp": "^0.18.1",
    "shuffle-array": "^1.0.0",
    "simple-git": "^1.66.0",
    "simple-lastfm": "https://github.com/Shuunen/simple-lastfm",
    "socket.io": "^2.0.2",
    "socket.io-client": "^2.0.2",
    "spdy": "^3.4.4"
  },
  "devDependencies": {},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "yarn && nodemon server.js",
    "client-good": "node client.js --musicIs=good",
    "client-bad": "node client.js --musicIs=bad",
    "dont-watch": "start server.js"
  },
  "jscsConfig": {
    "preset": "airbnb",
    "disallowTrailingComma": true,
    "fix": true,
    "maximumLineLength": 200,
    "requireLineFeedAtFileEnd": false,
    "requirePaddingNewLinesAfterBlocks": false,
    "requirePaddingNewLinesBeforeLineComments": false,
    "requireTrailingComma": false,
    "validateIndentation": 4
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shuunen/musitop.git"
  },
  "keywords": [
    "music",
    "top",
    "sort",
    "delete"
  ],
  "author": "Shuunen",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/Shuunen/musitop/issues"
  },
  "homepage": "https://github.com/Shuunen/musitop#readme"
}
